{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ],
    "paths": {
      "@spartan-ng/ui-accordion-helm": [
        "./src/app/components/ui/ui-accordion-helm/src/index.ts"
      ],
      "@spartan-ng/ui-avatar-helm": [
        "./src/app/components/ui/ui-avatar-helm/src/index.ts"
      ],
      "@spartan-ng/ui-button-helm": [
        "./src/app/components/ui/ui-button-helm/src/index.ts"
      ],
      "@spartan-ng/ui-dialog-helm": [
        "./src/app/components/ui/ui-dialog-helm/src/index.ts"
      ],
      "@spartan-ng/ui-formfield-helm": [
        "./src/app/components/ui/ui-formfield-helm/src/index.ts"
      ],
      "@spartan-ng/ui-icon-helm": [
        "./src/app/components/ui/ui-icon-helm/src/index.ts"
      ],
      "@spartan-ng/ui-input-helm": [
        "./src/app/components/ui/ui-input-helm/src/index.ts"
      ],
      "@spartan-ng/ui-menu-helm": [
        "./src/app/components/ui/ui-menu-helm/src/index.ts"
      ],
      "@spartan-ng/ui-popover-helm": [
        "./src/app/components/ui/ui-popover-helm/src/index.ts"
      ],
      "@spartan-ng/ui-progress-helm": [
        "./src/app/components/ui/ui-progress-helm/src/index.ts"
      ],
      "@spartan-ng/ui-scrollarea-helm": [
        "./src/app/components/ui/ui-scrollarea-helm/src/index.ts"
      ],
      "@spartan-ng/ui-separator-helm": [
        "./src/app/components/ui/ui-separator-helm/src/index.ts"
      ],
      "@spartan-ng/ui-select-helm": [
        "./src/app/components/ui/ui-select-helm/src/index.ts"
      ],
      "@spartan-ng/ui-spinner-helm": [
        "./src/app/components/ui/ui-spinner-helm/src/index.ts"
      ],
      "@spartan-ng/ui-tabs-helm": [
        "./src/app/components/ui/ui-tabs-helm/src/index.ts"
      ],
      "@spartan-ng/ui-toggle-helm": [
        "./src/app/components/ui/ui-toggle-helm/src/index.ts"
      ],
      "@spartan-ng/ui-typography-helm": [
        "./src/app/components/ui/ui-typography-helm/src/index.ts"
      ],
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
